import java.io.*;
import java.util.*;
import java.util.Scanner;
 
 class Main{
     public static void main(String[] args){
         int row;
         Scanner d=new Scanner(System.in);
         row=d.nextInt();
         for(int i=1;i<=row;i++)
         {
             for(int j=1;j<=i;j++){
                 System.out.print(j+" ");
             }
             System.out.println();
         }
         
     }
 }
