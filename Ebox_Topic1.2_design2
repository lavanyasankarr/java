import java.util.*;
class Main{
    public static void main(String[] args){
    Scanner sc=new  Scanner(System.in);
    int a,b,c;
    a=sc.nextInt();
    b=sc.nextInt();
    c=sc.nextInt();
    if(a+b+c ==180){
        if((a==90) ||(b==90) ||(c==90))
        System.out.println("Prize 2");
        else if((a==b)&&(b==c)&&(a==c))
        System.out.println("Prize 3");
        else
        System.out.println("Prize 1");
    }
    else
    {
        System.out.println("No Prize");
    }
}
}
