import java.util.ArrayList;
import java.util.Iterator;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;
import java.util.HashMap;

public class Java_array_problem3 {
	 static Java_array_problem3 ca = new Java_array_problem3();
	
	private HashMap<String, String> M1;
	
	public Java_array_problem3() {
		M1 = new HashMap<String, String>();
	}
	
	public HashMap<String, String> storeCountryCapital(String CountryName, String capital) {
		M1.put(CountryName, capital);
		return M1;
	}
	
	public String retrieveCapital(String CountryName) {
		return M1.get(CountryName);
	}
	
	public String retrieveCountry(String capitalName) {
		Set<Entry<String, String>> set = M1.entrySet();
		Iterator<Entry<String, String>> it = set.iterator();
		
		while (it.hasNext()) {
			Map.Entry<String, String> me = it.next();
			
			if (me.getValue().equals(capitalName))
				return me.getKey();
		}
		
		return null;
	}
	
	
	public ArrayList<String> toArrayList() {
		ArrayList<String> list = new ArrayList<>();
		
		Set<Entry<String, String>> set = M1.entrySet();
		Iterator<Entry<String, String>> it = set.iterator();
		
		while (it.hasNext()) {
			Map.Entry<String, String> me = it.next();
			list.add(me.getKey());
		}
		
		return list;
	}
	public HashMap<String, String> method_to_iterate() {
		HashMap<String, String> M2 = new HashMap<String, String>();
		
		Set<Entry<String, String>> set = M1.entrySet();
		Iterator<Entry<String, String>> it = set.iterator();
		
		while (it.hasNext()) {
			Map.Entry<String, String> me = it.next();
			M2.put(me.getValue(), me.getKey());
		}
		
		return M2;
	}
	
	 public static void main (String []args){
		ca. storeCountryCapital("India","Delhi");
		ca. storeCountryCapital("Japan","Tokyo");
		System.out.println( ca.retrieveCapital("India"));
		System.out.println( ca.retrieveCountry("Tokyo"));
		System.out.println(ca.method_to_iterate());
	 }
}
